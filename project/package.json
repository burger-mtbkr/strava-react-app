    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start:local": "env-cmd -f ./environments/.env.local react-scripts start",
    "start:dev": "env-cmd -f ./environments/.env.development react-scripts start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --transformIgnorePatterns \"node_modules/(?!react-leaflet)/\" --env=jsdom --coverage --watchAll=false",
    "test:premerge": "npm-run-all lint:fix test",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/reportWebVitals.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{ts,tsx}",
      "./src/index.tsx",
      "./src/serviceWorker.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 72,
        "branches": 67,
        "functions": 77,
        "lines": 72
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-mock-store": "^1.0.3",
    "ts-jest": "^29.0.3"
  }
}
