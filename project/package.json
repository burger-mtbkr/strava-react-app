    "history": "^5.3.0",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "leaflet": "^1.9.2",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.1.0",
    "react-moment": "^1.1.2",
    "react-redux": "^8.0.5",
